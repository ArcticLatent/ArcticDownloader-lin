{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Arctic ComfyUI Helper",
  "version": "0.1.1",
  "identifier": "io.github.ArcticHelper",
  "build": {
    "frontendDist": "dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Arctic ComfyUI Helper",
        "width": 1240,
        "height": 860,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "publisher": "Arctic Latent",
    "icon": [
      "icons/icon-32.png",
      "icons/icon-64.png",
      "icons/icon-128.png",
      "icons/icon-256.png",
      "icons/icon-512.png",
      "icons/icon.png",
      "icons/icon.ico"
    ],
    "targets": "all"
  }
}
